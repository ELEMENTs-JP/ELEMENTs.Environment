

<div class="@ContainerClass">
    <div class="@PageClass">
        @ChildContent
    </div>
</div>


@code {



    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public string ContainerClass { get; set; } = "";

    [Parameter]
    public string PageClass { get; set; } = "";
}
