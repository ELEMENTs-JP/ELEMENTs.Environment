


@if (Items != null)
{
    @if (Items.Count >= 1)
    {
        <ul class="@ListCSS" style="@( "list-style: circle !important; ")">

            @foreach (string entry in Items)
            {
                <li class="@ItemCSS"><span class="pe-1">&#9725;</span>@entry</li>
            }

        </ul>
    }
}




@code {



    [Parameter]
    public List<string> Items { get; set; } = new List<string>();

    [Parameter]
    public string ListCSS { get; set; } = "bg-white list-group circle";

    [Parameter]
    public string ItemCSS { get; set; } = "list-group-item p-2";

}
